{
  "manifest_version": 2,
  "name": "Urlando",
  "version": "0.5",
  "description": "Cycles through user defined URLs in user defined intervals. Useful as dashboard / wallboard.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "app": {
    "launch": {
      "local_path": "main.html",
      "container": "tab"
    }
  },
  "options_page": "options/index.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' 'unsafe-eval';"
}
